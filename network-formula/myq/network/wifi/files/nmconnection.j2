[connection]
id={{ ssid }}
uuid={{ uuid }}
type=wifi
interface-name={{ ifname }}
permissions=user:user:;

[wifi]
mac-address-blacklist=
mode=infrastructure
ssid={{ ssid }}

[wifi-security]
auth-alg=open
key-mgmt=wpa-psk
psk={{ secret }}

[ipv4]
dns-search=
method=auto

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto

[proxy]
